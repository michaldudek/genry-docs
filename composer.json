{
    "name": "michaldudek/genry-docs",
    "description": "PHP documentation generator for Genry that uses Twig.",
    "keywords": ["generator", "documentation", "twig", "html", "phpdoc", "docs"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michał Pałys-Dudek",
            "email": "michal@michaldudek.pl"
        }
    ],
    "require": {
        "michaldudek/genry": "dev-master",
        "sami/sami": "1.4.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "squizlabs/php_codesniffer": "~2.3",
        "phpmd/phpmd" : "~2.2"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Genry\\Docs\\": "src/"
        }
    }
}
